<!DOCTYPE html>
<html>
<head>
  <title>Palkanlaskenta</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="header">
	<div id="logo">
		<h1>EIR v.0.2a</h1>
	</div>

<p id="notice"><%= notice %></p>

<% [:info, :error].each do |key| %>
  <% if flash[key] %>
  <div class="<%= key %>" id="flash">
    <%= flash[key] %>
  </div>
  <% end %>
<% end %>

<%= yield %>


</body>
</html>
